{
  "name": "AdBlock Q",
  "version": "0.6",
  "description": "Removes annoying ads.",
  "permissions": ["webRequest", "webRequestBlocking", "http://*/", "https://*/"],
  "background": {
    "scripts": ["blocked_domains.js","background.js"]
  },
  "icons": {
      "128": "logo_128.png"
   },
	"content_scripts": [
  {
    "matches": ["<all_urls>"]
  }
	],
	"browser_action": {
    "default_popup": "popup.html"
  },
  
  "manifest_version": 2
}
